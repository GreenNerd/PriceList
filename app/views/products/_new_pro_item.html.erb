<!-- sku product type -->
<div class="form-group">
  <label for="pro-sku-type" class="col-sm-2 control-label">商品型号</label>
  <div class="col-sm-10">
    <%= sku.text_field :product_type, class: "form-control", id: "pro-sku-type", placeholder: "请输入商品型号" %>
  </div>
</div>
<!-- sku product inventory count -->
<div class="form-group">
  <label for="pro-sku-count" class="col-sm-2 control-label">商品库存</label>
  <div class="col-sm-10">
    <%= sku.text_field :inventory_count, class: "form-control", id: "pro-sku-count", placeholder: "请输入商品库存" %>
  </div>
</div>
<!-- sku product prices -->
<div class="form-group">
  <label for="pro-sku-prices" class="col-sm-2 control-label">库存价格</label>
  <div class="col-sm-10">
    <%= sku.text_field :prices, class: "form-control", id: "pro-sku-prices", placeholder: "请输入库存价格" %>
  </div>
</div>
<!-- sku product dimensions -->
<div class="form-group">
  <label for="pro-sku-disn" class="col-sm-2 control-label">商品规格</label>
  <div class="col-sm-10">
    <%= sku.text_area :dimensions, class: "form-control", id: "pro-sku-prices", placeholder: "请输入商品规格" %>
  </div>
</div>
<!-- sku product delete -->
<div class="form-group">
  <div class="col-sm-10">
      Delete: <%= sku.check_box :_destroy %>
  </div>
</div>
