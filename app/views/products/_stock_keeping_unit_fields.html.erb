<div class="well">
  <!-- sku product type -->
  <div class="form-group">
    <label class="col-sm-2 control-label">商品型号</label>
    <div class="col-sm-10">
      <%= f.text_field :product_type, class: "form-control", placeholder: "请输入商品型号" %>
    </div>
  </div>
  <!-- sku product inventory count -->
  <div class="form-group">
    <label class="col-sm-2 control-label">商品库存</label>
    <div class="col-sm-10">
      <%= f.text_field :inventory_count, class: "form-control", placeholder: "请输入商品库存" %>
    </div>
  </div>
  <!-- sku product prices -->
  <div class="form-group">
    <label class="col-sm-2 control-label">库存价格</label>
    <div class="col-sm-10">
      <%= f.text_field :prices, class: "form-control", placeholder: "请输入库存价格" %>
    </div>
  </div>
  <!-- sku product delete -->
  <div class="form-group">
    <div class="col-sm-2"></div>
    <div class="col-sm-10">
      <div style="float:right" calss="aremove">
        <%= link_to_remove_fields "删除", f %>
      </div>
    </div>
  </div>
</div>
